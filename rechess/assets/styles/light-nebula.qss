Board {
  qproperty-coord: #3a1066;
  qproperty-inner_border: #9d4ee6;
  qproperty-margin: #d395ff;
  qproperty-outer_border: #3a1066;
  qproperty-square_dark: #a346f2;
  qproperty-square_dark_lastmove: #9339e0;
  qproperty-square_light: #e9ccff;
  qproperty-square_light_lastmove: #b375e7;
}

QDialog {
  background-color: #c385f7;
  border: 1px solid #9d4ee6;
  border-radius: 4px;
}

QHeaderView {
  background-color: #c385f7;
  border-bottom: 1px solid #9d4ee6;
  color: #3a1066;
  font-size: 15px;
  font-weight: bold;
}

QLabel {
  color: #3a1066;
  font-size: 15px;
  font-weight: bold;
}

QLabel#engineAnalysis, #engineName, #gameNotifications, #humanName {
  background-color: #d395ff;
  border: 1px solid #9d4ee6;
  border-radius: 4px;
  qproperty-alignment: AlignCenter;
}

QLineEdit {
  background-color: #d395ff;
  border: 1px solid #9d4ee6;
  border-radius: 4px;
  color: #3a1066;
  font-size: 15px;
  font-weight: bold;
  max-width: 600px;
  min-width: 600px;
  qproperty-alignment: AlignCenter;
}

QLineEdit:focus {
  background-color: #c385f7;
  border: 1px solid #3a1066;
}

QLineEdit:hover {
  background-color: #c98af9;
  border: 1px solid #8832d9;
}

QMainWindow {
  background-color: #c385f7;
  border: 1px solid #9d4ee6;
}

QMenu {
  background-color: #d395ff;
  border: 1px solid #9d4ee6;
  border-radius: 4px;
  font-weight: bold;
  padding: 6px 6px 6px 10px;
}

QMenu::item {
  background-color: transparent;
  color: #3a1066;
  margin: 2px;
  padding: 4px 20px 4px 20px;
}

QMenu::item:selected {
  background-color: #b375e7;
  border-radius: 2px;
}

QMenu::separator {
  background-color: #9d4ee6;
  height: 1px;
  margin: 8px;
}

QMenuBar {
  background-color: #c385f7;
  border-bottom: 1px solid #9d4ee6;
  font-weight: bold;
}

QMenuBar::item:selected {
  background-color: #b375e7;
}

QPushButton {
  background-color: #9d4ee6;
  border-radius: 4px;
  color: #ffffff;
  font-weight: bold;
  padding: 8px 16px;
}

QPushButton:disabled {
  background-color: #b375e7;
  color: #d395ff;
}

QPushButton:hover {
  background-color: #8832d9;
}

QPushButton:pressed {
  background-color: #3a1066;
}

QProgressBar {
  background-color: white;
  border: 1px solid #9d4ee6;
  border-radius: 4px;
  color: #9d4ee6;
  font-size: 13px;
  font-weight: bold;
  text-align: center;
}

QProgressBar::chunk {
  background-color: black;
  border-radius: 3px;
}

QScrollBar:horizontal {
  background-color: #c385f7;
  height: 14px;
  margin: 0px;
}

QScrollBar:vertical {
  background-color: #c385f7;
  margin: 0px;
  width: 14px;
}

QScrollBar::handle:horizontal {
  background-color: #b375e7;
  border-radius: 7px;
  margin: 2px;
  min-width: 24px;
}

QScrollBar::handle:horizontal:hover {
  background-color: #9d4ee6;
}

QScrollBar::handle:vertical {
  background-color: #b375e7;
  border-radius: 7px;
  margin: 2px;
  min-height: 24px;
}

QScrollBar::handle:vertical:hover {
  background-color: #9d4ee6;
}

QStatusBar {
  background-color: #c385f7;
  border-top: 1px solid #9d4ee6;
  color: #3a1066;
}

QStatusBar QLabel {
  padding-left: 4px;
  padding-right: 4px;
}

QTableView {
  background-color: #d395ff;
  border: 1px solid #9d4ee6;
  border-radius: 4px;
  color: #3a1066;
  font-size: 15px;
  font-weight: bold;
  gridline-color: #9d4ee6;
  selection-background-color: #b375e7;
}

QTableView::item:hover {
  background-color: #b375e7;
}

QToolBar {
  background-color: #c385f7;
  border-bottom: 1px solid #9d4ee6;
  padding: 4px;
  spacing: 4px;
}

QToolTip {
  background-color: #c385f7;
  border: 1px solid #9d4ee6;
  border-radius: 4px;
  color: #3a1066;
  padding: 6px;
}
