Board {
  qproperty-coord: #c5d9e8;
  qproperty-inner_border: #385f80;
  qproperty-margin: #344d69;
  qproperty-outer_border: #385f80;
  qproperty-square_dark: #2d4766;
  qproperty-square_dark_lastmove: #1d3756;
  qproperty-square_light: #9ab8d9;
  qproperty-square_light_lastmove: #7ea0c7;
}

QDialog {
  background-color: #2e455e;
  border: 1px solid #385f80;
  border-radius: 4px;
}

QHeaderView {
  background-color: #2e455e;
  border-bottom: 1px solid #385f80;
  color: #c5d9e8;
  font-size: 15px;
  font-weight: bold;
}

QLabel {
  color: #c5d9e8;
  font-size: 15px;
  font-weight: bold;
}

QLabel#analysis {
  background-color: #344d69;
  border: 1px solid #385f80;
  border-radius: 4px;
  min-height: 80px;
}

QLabel#engine, QLabel#human, QLabel#notifications {
  background-color: #344d69;
  border: 1px solid #385f80;
  border-radius: 4px;
  min-height: 20px;
  qproperty-alignment: "AlignCenter";
}

QLineEdit {
  background-color: #344d69;
  border: 1px solid #385f80;
  border-radius: 4px;
  color: #c5d9e8;
  font-size: 15px;
  font-weight: bold;
  min-height: 20px;
  qproperty-alignment: "AlignCenter";
}

QLineEdit:focus {
  background-color: #2e455e;
  border: 1px solid #4d7099;
}

QLineEdit:hover {
  background-color: #3a5574;
  border: 1px solid #4d7099;
}

QMainWindow {
  background-color: #2e455e;
  border: 1px solid #385f80;
}

QMenu {
  background-color: #344d69;
  border: 1px solid #385f80;
  border-radius: 4px;
  font-weight: bold;
  padding: 6px 6px 6px 10px;
}

QMenu::item {
  background-color: transparent;
  color: #c5d9e8;
  margin: 2px;
  padding: 4px 20px 4px 20px;
}

QMenu::item:selected {
  background-color: #385f80;
  border-radius: 2px;
}

QMenu::separator {
  background-color: #385f80;
  height: 1px;
  margin: 8px;
}

QMenuBar {
  background-color: #2e455e;
  border-bottom: 1px solid #385f80;
  font-weight: bold;
}

QMenuBar::item:selected {
  background-color: #385f80;
}

QPushButton {
  background-color: #385f80;
  border-radius: 4px;
  color: #c5d9e8;
  font-weight: bold;
  padding: 8px 16px;
}

QPushButton:disabled {
  background-color: #344d69;
  color: #6b8599;
}

QPushButton:hover {
  background-color: #4d7099;
}

QPushButton:pressed {
  background-color: #344d69;
}

QProgressBar {
  background-color: white;
  border: 1px solid #385f80;
  border-radius: 4px;
  color: #385f80;
  font-size: 13px;
  font-weight: bold;
  text-align: center;
}

QProgressBar::chunk {
  background-color: black;
  border-radius: 3px;
}

QScrollBar:horizontal {
  background-color: #2e455e;
  height: 14px;
  margin: 0px;
}

QScrollBar:vertical {
  background-color: #2e455e;
  margin: 0px;
  width: 14px;
}

QScrollBar::handle:horizontal {
  background-color: #385f80;
  border-radius: 7px;
  margin: 2px;
  min-width: 24px;
}

QScrollBar::handle:horizontal:hover {
  background-color: #4d7099;
}

QScrollBar::handle:vertical {
  background-color: #385f80;
  border-radius: 7px;
  margin: 2px;
  min-height: 24px;
}

QScrollBar::handle:vertical:hover {
  background-color: #4d7099;
}

QStatusBar {
  background-color: #2e455e;
  border-top: 1px solid #385f80;
  color: #c5d9e8;
}

QTableView {
  background-color: #344d69;
  border: 1px solid #385f80;
  border-radius: 4px;
  color: #c5d9e8;
  font-size: 15px;
  font-weight: bold;
  gridline-color: #385f80;
  selection-background-color: #385f80;
}

QTableView::item:hover {
  background-color: #385f80;
}

QToolBar {
  background-color: #2e455e;
  border-bottom: 1px solid #385f80;
  padding: 4px;
  spacing: 4px;
}

QToolTip {
  background-color: #2e455e;
  border: 1px solid #385f80;
  border-radius: 4px;
  color: #c5d9e8;
  padding: 6px;
}
